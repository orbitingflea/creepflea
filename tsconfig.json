{
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "es2018"
    ],
    "target": "es2018",
    "moduleResolution": "Node",
    "outDir": "dist",
    "baseUrl": "src/",
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowJs": true,
    "paths": {
      "goog:*": [
        "../node_modules/closure-library.d.ts/index.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
